{
  "name": "communityos-frontend",
  "version": "1.0.0",
  "description": "CommunityOS Frontend Dashboard - React/Next.js application for Discord community management",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "health": "curl -f http://localhost:3000 || exit 1",
    "docker:build": "docker build -t communityos-frontend .",
    "deploy:staging": "echo 'Deploy to staging'",
    "deploy:prod": "echo 'Deploy to production'"
  },
